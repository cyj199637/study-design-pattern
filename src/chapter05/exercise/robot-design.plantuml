@startuml
abstract class Robot {
    -name: String
    +Robot(name: String)
    +getName(): String
    +{abstract}attack(): void
    +{abstract}move(): void
}

class TaekwonV {
    +TaekwonV(name: String)
    +attack(): void
    +move(): void
}

class Atom {
    +Atom(name: String)
    +attack(): void
    +move(): void
}

Robot <|-- TaekwonV
Robot <|-- Atom
@enduml